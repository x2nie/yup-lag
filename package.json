{
  "name": "yup-notebook",
  "displayName": "Yup Notebook",
  "description": "Notebook using Serializer API sample",
  "publisher": "x2nie",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onNotebook:yup-notebook-serializer"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "yup-notebook.createJsonNotebook",
        "title": "Create YUP Notebook"
      }
    ],
    "notebooks": [
      {
        "type": "yup-notebook-serializer",
        "displayName": "Yup DN Notebook",
        "selector": [
          {
            "filenamePattern": "*.sample-yup-notebook"
          }
        ]
      }
    ],
    "notebookRenderer": [
      {
        "id": "yup-notebook-renderer-ipstate",
        "entrypoint": "./static/notebook-renderers/ipstate.js",
        "displayName": "Yup Finish Renderer",
        "mimeTypes": [
          "x-application/yup-ipstate+json"
        ]
      }
    ],
    "languages": [
      {
        "id": "yup",
        "extensions": [
          ".yu",
          ".yu.xml",
          ".yup",
          ".yup.xml"
        ],
        "aliases": [
          "Yup"
        ],
        "configuration": "./syntax/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "yup",
        "scopeName": "source.yup",
        "path": "./syntax/yup.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "lint": "eslint src --ext ts",
    "watch": "tsc -b --watch",
    "dev": "vite"
  },
  "keywords": [],
  "author": "Yuu6883",
  "license": "MIT",
  "devDependencies": {
    "@rgrove/parse-xml": "^4.1.0",
    "@types/ace": "^0.0.48",
    "@types/file-saver": "^2.0.5",
    "@types/mocha": "^10.0.8",
    "@types/node": "^22.7.5",
    "@types/seedrandom": "^3.0.2",
    "@types/vscode": "^1.74.0",
    "@types/vscode-notebook-renderer": "^1.72.3",
    "@typescript-eslint/eslint-plugin": "^7.14.0",
    "@typescript-eslint/parser": "^7.14.0",
    "ace-builds": "^1.8.1",
    "eslint": "^8.56.0",
    "gl-matrix": "^3.4.3",
    "glslify": "^7.1.1",
    "glslify-loader": "^2.0.0",
    "regl": "^2.1.0",
    "regl-atmosphere-envmap": "^1.0.5",
    "typescript": "^5.5.2",
    "vite": "^5.4.8"
  },
  "dependencies": {}
}
